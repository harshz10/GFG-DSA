//C++
bool areIdentical(struct Node *head1, struct Node *head2) {
    // Code here
    while(head1 and head2){
    if(head1->data != head2->data)
       return false;
       //check for next value at link list 
       head1 = head1->next;
       head2 = head2->next;
       
    }
    //if one link list bigger than other 
    if(head1 or head2 )
      return false;
      
      return true;
       
}
