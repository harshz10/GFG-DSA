//c++
bool search(Node* root, int target) {
    // Your code here
    {
        if(!root)
          return 0;
        
        if(root->data==target)
          return 1;
          
        if(root->data>target)
        return search(root->left,target);
        
        if(root->data<target)
        return search(root->right,target);
          
        
    }
}
